@page
@model PrivacyModel
@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>

<h2>Mina user roller</h2>
<ul>
    @if (Model.MyRoles != default)
    {
        foreach (var role in Model.MyRoles)
        {
            <li>@role</li>
        }
    }
</ul>
